    "@heroicons/react": "^2.0.18",
    "@media/tailwind-config": "^0.1.0",
    "@mui/x-date-pickers": "^6.18.2",
    "dayjs": "^1.11.10",
    "formik": "^2.4.5",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@media/tsconfig": "^0.1.0",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.10",
    "@types/styled-components": "^5.1.32",
    "autoprefixer": "^10.4.16",
    "postcss": "8.4.31",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^6.1.1",
    "tailwindcss": "3.3.2",
    "tsup": "^6.0.1",
    "turbo": "^1.10.16",
    "typescript": "^5.3.2",
    "vite": "^5.0.0"
  }
